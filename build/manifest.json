{
  "manifest_version": 3,
  "name": "Google Image Crawler",
  "version": "1.0.0",
  "description": "Google Image Crawler",
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Google Image Crawler",
    "default_icon": "icons/logo.png",
    "default_popup": "popup.html"
  },

  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["googleScript.js"],
      "run_at": "document_end"
    }
  ]
}
